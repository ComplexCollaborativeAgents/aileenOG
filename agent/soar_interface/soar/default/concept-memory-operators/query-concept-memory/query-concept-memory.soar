sp {query-concept-memory*write-to-output-link
    (state <s>    ^operator <op>
                  ^io.output-link <ol>)
    (<op>    ^name query-concept-memory
             ^pattern <pat>
             ^facts <facts>)
-->
    (<ol>    ^concept-memory.query <query>)
    (<query>    ^pattern <pat>
                ^facts <facts>)
    }

sp {query-concept-memory*read-results*success
    (state <s>    ^operator <op>
                  ^superstate <ss>
                  ^io.input-link <il>)
    (<op>    ^name query-concept-memory
             ^pattern <pat>
             ^facts <facts>)
    (<il>    ^concept-memory <cm>)
    (<cm>    ^status success
             ^matches <m>)
-->
    (<ss>    ^retrieved <m>)
    }

sp {query-concept-memory*read-results*failed
    (state <s>    ^operator <op>
                  ^superstate <ss>
                  ^io.input-link <il>)
    (<op>    ^name query-concept-memory
             ^pattern <pat>
             ^facts <facts>)
    (<il>    ^concept-memory <cm>)
    (<cm>    ^status failure)
-->
    (<ss>    ^retrieved failure)
    }
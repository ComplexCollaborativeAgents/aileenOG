                  ^operator <op>
                  ^objects.object <obj>
                  ^superstate <ss>
                  ^pair <p>)
    (<p>    ^concept <con>
            ^token <token>)
    (<op>    ^name complete-generate-for-object)
    (<ss>    ^information.objects.for <for>)
    (<for>    ^object <obj>)
    (<obj>    ^isa <con>)
-->
    (<for>    ^token <token>)
}

sp {generate-for-object*apply*complete-generate-for-object*completed
    (state <s>    ^name generate-for-object
                  ^operator.name complete-generate-for-object
                  ^superstate <ss>)
    (<ss>    ^information.objects.for.token <any>
             ^for-concept object)
-->
    (<ss>    ^completed-for-concept object)
    }
    (<ss>    ^information.objects <oinfo>)
    (<obj>    ^id_string <id>)
-->
    (<oinfo>    ^for <for>)
    (<for>    ^id <id>
              ^object <obj>)
}

sp {generate-for-object*apply*complete-generate-for-object*tokens

sp {generate-for-object*elaborate*state
    (state <s>    ^name generate-for-object
                  ^superstate.operator <op>)
    (<op>    ^name generate-for-object
             ^objects <obj>)
-->
    (<s>    ^objects <obj>)
    }

sp {generate-for-object*elaborate*superstate*information
    (state <s>    ^name generate-for-object
                  ^superstate.information <info>)
-->
    (<info>    ^objects <objs>)
    }

sp {generate-for-object*elaborate*segment
    (state <s>    ^name generate-for-object
                  ^superstate.segment <seg>)
-->
    (<s>    ^segment <seg>)
    }

sp {generate-for-object*elaborate-with-concept*object*matches
    (state <s>    ^name generate-for-object
                  ^operator <op>
                  ^io.input-link.concept-memory.result <cm>
                  ^objects.object <obj>)
    (<obj>    ^id_string <id-name>)
    (<cm>    ^matches.match <m>)
    (<m>    ^first isa
            ^second <id-name>
            ^third <val>)
    (<op>    ^name describe-subscene)
-->
    (<obj>    ^isa <val>)
    }

sp {generate-for-object*elaborate-with-concept*status*failure*concept-memory
    (state <s>    ^name generate-for-object
                  ^operator <op>
                  ^io.input-link.concept-memory.result <cm>
                  ^objects.object <obj>)
    (<cm>    ^status success
             -^matches.match)
-->
    (<s>    ^memory-lookup-status failed)
    }
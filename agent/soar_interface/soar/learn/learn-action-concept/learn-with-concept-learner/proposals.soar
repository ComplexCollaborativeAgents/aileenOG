sp {learn-action-with-concept-learner*propose*store-new-concept
    (state <s>    ^name learn-action-with-concept-learner
                  ^concept-structure <con-s>)
    (<con-s>    -^gpool)
-->
    (<s>    ^operator <op> +)
    (<op>    ^name store-new-concept
             ^concept-structure <con-s>)
    }


sp {learn-action-with-concept-learner*propose*gather-action-trace
    (state <s>    ^name learn-action-with-concept-learner
                  ^concept-structure <struct>
                  -^trace <t>)
    (<struct>    ^gpool)
-->
    (<s>    ^operator <op> + =)
    (<op>    ^name gather-trace
             ^concept-structure <struct>)
    }

sp {learn-action-with-concept-learner*propose*augment-trace-with-action
    (state <s>    ^name learn-action-with-concept-learner
                  ^trace <t>)
 #                 ^concept-structure.)
    (<t>    -^action <act>)
-->
    (<s>    ^operator <op> +)
    (<op>    ^name augment-trace-with-action
             ^trace <t>)
    }


sp {learn-action-with-concept-learner*propose*compile-facts
    (state <s>    ^name learn-action-with-concept-learner
                  ^trace <t>
                  -^facts <facts>)
-->
    (<s>    ^operator <op> +)
    (<op>    ^name compile-facts)
    }


sp {learn-action-with-concept-learner*elaborate*state
    (state <s>    ^name learn-action-with-concept-learner
                  ^superstate.operator <sop>)
    (<sop>    ^name learn-action-with-concept-learner
              ^information <info>)
-->
    (<s>    ^information <info>)
    }

sp {learn-action-with-concept-learner*elaborate*parses*grounding
    (state <s>    ^name learn-action-with-concept-learner
                  ^information <info>)
    (<info>    ^grounding <g>
               ^parses <p>
               ^nl <nl>)
-->
    (<s>    ^grounding <g>
            ^parses <p>
            ^nl <nl>)
    }

sp {learn-action-with-concept-learner*store*action-word*index
    (state <s>    ^name learn-action-with-concept-learner
                  ^nl <tag>)
-->
    (<s>    ^concept-structure <index>)
    (<index>    ^concept (make-constant-symbol r_ <tag>)
                ^type action
                ^nl <tag>)
    }

sp {learn-action-with-concept-learner*store*map*semantic-memory
    (state <s>    ^name learn-action-with-concept-learner
                  ^concept-structure <struct>)
    (<struct>    ^concept <con>
                 ^type <type>
                 ^nl <nl>
                 ^gpool <gpool>)
-->
    (<s>    ^store-concept <struct>)
    }
                  



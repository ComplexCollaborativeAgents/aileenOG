sp {gather-trace*apply*describe-state-action-start
    (state <s>    ^name gather-trace
                  ^world <w>
                  ^trace <t>
                  ^operator <op>)
    (<op>    ^name record-state
             ^label <lab>)
    (<w>     ^relations <rels>)
-->
    (<t>    ^state-record <as>)
    (<as>    ^relations <rel-new>
             ^label <lab>)
    }

sp {gather-trace*apply*describe-state-action-starte*relation
    (state <s>    ^name gather-trace
                  ^world <w>
                  ^trace <t>
                  ^operator <op>)
    (<op>    ^name record-state
             ^label <lab>)
    (<t>    ^state-record <tsr>)
    (<tsr> ^relations <rel-as>
           ^label <lab>)
    (<w>    ^relations.relation <rel>)
-->
    (<rel-as> ^relation <rel>)
    }


sp {action*propose*pick-up
    (state <s>    ^name action
                  ^world.objects.object <obj>)
    (<obj>    ^held false)
-->
    (<s>    ^operator <op> +)
    (<op>    ^name op_pick-up
             ^argument1 <obj>)
    }

sp {action*apply*pick-up
    (state <s>    ^name action
                  ^operator <op>
                  ^io.output-link <ol>)
    (<op>    ^name op_pick-up
             ^argument1 <obj>)
    (<obj>    ^id <id>)
-->
    (<ol>    ^action <act>)
    (<act>    ^name pick-up
              ^id <id>)
}
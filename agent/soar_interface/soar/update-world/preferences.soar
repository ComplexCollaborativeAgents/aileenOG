sp {prefer*update-world*over*any*other
    (state <s>    ^name aileen
                  ^operator <op1> +
                  ^operator <op2> +)
    (<op1>    ^name update-world)
    (<op2>    -^name update-world)
-->
    (<s>    ^operator <op1> > <op2>)
    }

sp {prefere*update-world*equal
    (state <s>    ^name aileen
                  ^operator <op1> +
                  ^operator <op2> +)
    (<op1>    ^name update-world)
    (<op2>    ^name update-world)
-->
    (<s>    ^operator <op1> = <op2>)
    }
                  
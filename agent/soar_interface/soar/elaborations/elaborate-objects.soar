sp {aileen*elaborate*objects*copy*id
    :o-support
    (state <s>    ^name aileen
                  ^io.input-link <il>
                  ^world.objects <wobjs>)
    (<il>    ^world.objects.object <obj>)
    (<obj>  ^id <id>)
   -{(<wobjs>    ^object <wobj>)
    (<wobj>    ^id <id>)}
-->
    (<wobjs>    ^object <obj-new>)
    (<obj-new> ^id <id>)
    }

sp {aileen*elaborate*objects*copy*attributes
    :o-support
    (state <s>    ^name aileen
                  ^io.input-link.world.objects.object <ilo>
                  ^world.objects.object <obj>)
    (<ilo>    ^id <id>
              ^<attr> <val>)
    (<obj>    ^id <id>)
-->
    (<obj>    ^<attr> <val>)
    }

sp {aileen*elaborate*objects*remove
    :o-support
    (state <s>    ^name aileen
                  ^io.input-link <il>
                  ^world.objects <wobjs>)
    -{(<il>    ^world.objects.object <obj>)
    (<obj>    ^id <id>)}
    (<wobjs>    ^object <wobj>)
    (<wobj>    ^id <id>)
-->
    (<wobjs>    ^object  <wobj> -)
    }

sp {aileen*elaborate*objects*position
    (state <s>    ^name aileen
                  ^io.input-link <il>
                  ^world.objects.object <wobj>)
    (<il>    ^world.objects.object <obj>)
    (<obj>    ^id <id>
              ^position <p>)
    (<wobj>    ^id <id>)
-->
    (<wobj>    ^position <p>)
    }

sp {aileen*elaborate*objects*update*held
    :o-support
    (state <s>    ^name aileen
                  ^io.input-link <il>
                  ^world.objects.object <wobj>)
    (<il>    ^world.objects.object <obj>)
    (<obj>    ^id <id>
              ^held <held>)
    (<wobj>    ^id <id>
               ^held {<nheld> <> <held>})
-->
    (<wobj>    ^held <nheld> -
               ^held <held>)
    }

sp {aileen*elaborate*object*holdInHand
    (state <s>    ^name aileen
                  ^world <w>)
    (<w>    ^objects.object <obj>)
    (<obj>  ^held true)
-->
    (<w>    ^holdsInHand <obj>)
    }

sp {aileen*elaborate*add*qsrs
    :o-support
    (state <s>    ^name aileen
                  ^io.input-link <il>
                  ^world <w>)
    (<w>    ^objects <objs>
            ^relations <rels>)
    (<il>    ^world.qsrs.qsr <qsr>)
    (<qsr>    ^root <root-num>
              ^target <target-num>
              ^{<< cardir rcc8 >> <type>} <val>)
    (<objs>    ^object <root-obj>
               ^object <target-obj>)
    (<root-obj>    ^id <root-num>)
    (<target-obj>    ^id <target-num>)
    -{(<rels>    ^relation <rel>)
      (<rel>    ^root-object <root-obj>
                ^target-object <target-obj>
                ^<type> <val>)}
-->
    (<rels>    ^relation <rel-new>)
    (<rel-new>    ^root-object <root-obj>
                  ^target-object <target-obj>
                  ^<type> <val>)
    }

sp {aileen*elaborate*remove*qsrs*no-root-object
    :o-support
    (state <s>    ^name aileen
                  ^world <w>)
    (<w>    ^objects <objs>
            ^relations <rels>)
    (<rels>    ^relation <rel>)
    (<rel>    ^root-object <root-obj>
              ^target-object <target-obj>
              ^<< cardir rcc8 >> <val>)
    (<objs>    -^object <root-obj>)
-->
    (<rels>    ^relation <rel> -)
    }

sp {aileen*elaborate*remove*qsrs*no-target-object
    :o-support
    (state <s>    ^name aileen
                  ^world <w>)
    (<w>    ^objects <objs>
            ^relations <rels>)
    (<rels>    ^relation <rel>)
    (<rel>    ^root-object <root-obj>
              ^target-object <target-obj>
              ^<< cardir rcc8 >> <val>)
    (<objs>    -^object <target-obj>)
-->
    (<rels>    ^relation <rel> -)
    }

sp {aileen*elaborate*remove*qsrs*different-qsr
    :o-support
    (state <s>    ^name aileen
                  ^world <w>
                  ^io.input-link.world.qsrs.qsr <irel>)
    (<w>    ^objects <objs>
            ^relations <rels>)
    (<rels>    ^relation <rel>)
    (<rel>    ^root-object <root-obj>
              ^target-object <target-obj>
              ^{<< cardir rcc8 >> <type>} <val>)
    (<objs>    ^object <target-obj> <root-obj>)
    (<irel>    ^root <root-num>
               ^target <target-num>
               ^<type> {<nval> <> <val>})
    (<root-obj>    ^id <root-num>)
    (<target-obj>    ^id <target-num>)
-->
    (<rels>    ^relation <rel> -)
    }

sp {aileen*elaborate*remove*qsrs*no-qsr
    :o-support
    (state <s>    ^name aileen
                  ^world <w>
                  ^io.input-link.world.qsrs <qsrs>)
    (<w>    ^objects <objs>
            ^relations <rels>)
    (<rels>    ^relation <rel>)
    (<rel>    ^root-object <root-obj>
              ^target-object <target-obj>
              ^{<< cardir rcc8 >> <type>} <val>)
    (<objs>    ^object <target-obj> <root-obj>)
    -{(<qsrs>  ^qsr <q>)
       (<q>    ^root <root-num>
               ^target <target-num>)}
-->
    (<rels>    ^relation <rel> -)
    }
      
              

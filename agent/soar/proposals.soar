sp {aileen*propose*interaction
    (state <s>    ^name aileen
                  ^interaction <inter>)
    (<inter> -^complete true)
-->
    (<s>    ^operator <op> +)
    (<op>    ^name interaction
             ^interaction <inter>)
}

sp {aileen*propose*comprehension
    (state <s>    ^name aileen
                  ^interaction <inter>)
    (<inter>    ^parses <p>
                -^grounds <g>)
-->
    (<s>    ^operator <op> +)
    (<op>    ^name comprehension
             ^parses <p>)
    }


sp {aileen*propose*verify
    (state <s>    ^name aileen
                  ^interaction <inter>)
    (<inter>    ^signal verify
                ^grounds <g>
                ^content <cont>
                ^parses <p>
                -^verification)
-->
    (<s>    ^operator <op> +)
    (<op>    ^name verify)
    }

sp {aileen*propose*learn
    (state <s>    ^name aileen
                  ^learn <learn>)
-->
    (<s>    ^operator <op> +)
    (<op>    ^name learn
             ^details <learn>)
    }
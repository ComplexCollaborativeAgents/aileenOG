smem --add {
    (<map_blue> ^nl blue ^world <filter_blue> ^type visual) # adding world-to-word map for color blue
    (<filter_blue>    ^percept cv_blue)
    
    (<map_red> ^nl red ^world <filter_red> ^type visual)
    (<filter_red>    ^percept cv_red)
    
    (<map_green> ^nl green ^world <filter_green> ^type visual)
    (<filter_green>    ^percept cv_green)
    
    (<map_cone> ^nl cone ^world <filter_cone> ^type visual)
    (<filter_cone>    ^percept cv_cone)
    
    (<map_cylinder> ^nl red ^world <filter_cylinder> ^type visual)
    (<filter_cylinder>    ^percept cv_cylinder)
    
    (<map_box> ^nl box ^world <filter_box> ^type visual)
    (<filter_box>    ^percept cv_box)
    
    (<map_sphere> ^nl red ^world <filter_sphere> ^type visual)
    (<filter_sphere>    ^percept cv_sphere)
}
sp {interaction*apply*update-stack*message*new-segment
    (state <s>    ^name interaction
                  ^operator <op>
                  ^interaction.stack <stack>
                  ^new-segment <new-seg>
                  -^terminate-segment <seg>)
    (<stack>    ^segment <seg>)
    (<op>    ^name update-stack)
    (<new-seg>    ^message <m>)
-->
    (<new-seg>    ^next <seg>)
    (<stack>    ^segment <new-seg>
                ^segment <seg> -)
    (<m>    ^processed true)
    }

sp {interaction*apply*update-stack*terminate-segment*only*status
    (state <s>    ^name interaction
                ^operator.name update-stack
                 ^interaction <inter>
                  ^terminate-segment <seg>
                   -^new-segment <new-seg>)
     (<inter>    ^stack <stack>)
     (<stack>    ^segment <seg>)
      (<seg>    ^next <next-seg>
                ^message <m>)
      (<m>    -^processed true)
-->
    (<stack>    ^segment <next-seg>
                  ^segment <seg> -) 
    (<m>    ^processed true)
  }
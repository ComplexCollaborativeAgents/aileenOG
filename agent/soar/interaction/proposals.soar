sp {interaction*propose*parse
    (state <s>    ^name interaction
                  ^content <cont>
                  -^parses <parse>)
-->
    (<s>    ^operator <op> +)
    (<op>    ^name parse-content
             ^content <cont>)
    }


sp {interaction*propose*complete-interaction
    (state <s>    ^name interaction
                  ^signal verify
                  ^verification success)
-->
    (<s>    ^operator <op> +)
    (<op>    ^name complete-interaction)
    }
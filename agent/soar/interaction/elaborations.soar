sp {interaction*elaborate*state*message
    (state <s>    ^name interaction
                  ^superstate.operator <sop>)
    (<sop>    ^name interaction
              ^message <m>)
-->
    (<s>    ^message <m>)
    }

sp {interaction*elaborate*state*interaction
    (state <s>    ^name interaction
                  ^superstate.operator <sop>)
    (<sop>    ^name interaction
              ^interaction <inter>)
-->
    (<s>    ^interaction <inter>)
    }


sp {topstate*delete*processed*response
    (state <s>    ^name interaction
                  ^topstate <ts>)
    (<ts>    ^response <res>)
    (<res>    ^processed true)
-->
    (<ts>    ^response <res> -)
    }